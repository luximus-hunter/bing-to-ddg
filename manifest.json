{
  "manifest_version": 2,
  "name": "Bing to DuckDuckGo",
  "version": "1.0.0",
  "description": "This extension redirects all Bing search results to DuckDuckGo",
  "icons": {
    "16": "icon-16x.png",
    "32": "icon-32x.png",
    "48": "icon-48x.png",
    "128": "icon-128x.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
